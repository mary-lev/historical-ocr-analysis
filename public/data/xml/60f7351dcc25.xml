                    TAGREFS="LT74"
                    BASELINE="315 1062 1694 1062" 
                    HPOS="314"
                    VPOS="1033"
                    WIDTH="1380"
                    HEIGHT="59">
            <Shape><Polygon POINTS="315 1033 1694 1035 1694 1092 314 1090"/></Shape>
	    <String CONTENT="горлица, пѣли, свистали, кричали, ворковали, пры-"
                    HPOS="314"
                    VPOS="1033"
                    WIDTH="1380"
                    HEIGHT="59"></String>
          </TextLine>
          <TextLine ID="line_11"
                    TAGREFS="LT74"
                    BASELINE="315 1146 1691 1146" 
