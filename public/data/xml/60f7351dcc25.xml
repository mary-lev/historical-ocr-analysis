                    TAGREFS="LT74"
                    BASELINE="318 1225 1459 1225" 
                    HPOS="318"
                    VPOS="1197"
                    WIDTH="1141"
                    HEIGHT="57">
            <Shape><Polygon POINTS="318 1197 1459 1197 1459 1254 318 1254"/></Shape>
	    <String CONTENT="бы всѣ онѣ между собою разговаривали."
                    HPOS="318"
                    VPOS="1197"
                    WIDTH="1141"
                    HEIGHT="57"></String>
          </TextLine>
          <TextLine ID="line_13"
                    TAGREFS="LT77"
