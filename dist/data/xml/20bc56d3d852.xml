                    TAGREFS="LT74"
                    BASELINE="426 1261 2364 1261" 
                    HPOS="426"
                    VPOS="1140"
                    WIDTH="1938"
                    HEIGHT="121">
            <Shape><Polygon POINTS="426 1140 2364 1140 2364 1261 426 1261"/></Shape>
	    <String CONTENT="радостїю нечистою? Плакать должно, и воздыхать"
                    HPOS="426"
                    VPOS="1140"
                    WIDTH="1938"
                    HEIGHT="121"></String>
          </TextLine>
          <TextLine ID="line_7"
                    TAGREFS="LT74"
                    BASELINE="426 1344 2366 1344" 
                    HPOS="426"
                    VPOS="1222"
                    WIDTH="1940"
                    HEIGHT="122">
            <Shape><Polygon POINTS="426 1222 2366 1222 2366 1344 426 1344"/></Shape>
	    <String CONTENT="о прошедшемъ! Пѣсни блудничи ты поешь, отверг-"
                    HPOS="426"
                    VPOS="1222"
                    WIDTH="1940"
                    HEIGHT="122"></String>
          </TextLine>
          <TextLine ID="line_8"
                    TAGREFS="LT74"
                    BASELINE="424 1430 2371 1430" 
