            <Shape><Polygon POINTS="429 974 2362 974 2362 1097 429 1097"/></Shape>
	    <String CONTENT="срамныя, знаки блудныя, возбуждающїя къ любодѣ-"
                    HPOS="429"
                    VPOS="974"
                    WIDTH="1933"
                    HEIGHT="123"></String>
          </TextLine>
